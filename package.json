  },
  "devDependencies": {
    "jsdom": "^15.1.1",
    "mocha": "^6.1.4",
    "monaco-editor-core": "0.18.1",
    "monaco-plugin-helpers": "^1.0.2",
    "requirejs": "^2.3.6",
    "typescript": "3.5.3",
    "uglify-js": "^3.6.0"
